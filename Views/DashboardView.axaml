				</controls:Card>
				<controls:Card Width="532" Height="265" Margin="8">
					<lvc:CartesianChart Series="{Binding Series}" YAxes="{Binding YAxes}" />
				</controls:Card>
				<controls:Card Width="532" Height="256" Margin="8">
